{
  "kind": "collectionType",
  "collectionName": "tourists",
  "info": {
    "name": "Tourists",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "maxLength": 255,
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "lastname": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "birth_date": {
      "type": "date",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "M",
        "F",
        "O"
      ],
      "required": true
    },
    "city_id": {
      "model": "cities",
      "via": "tourists"
    },
    "visits": {
      "via": "tourist_id",
      "autoPopulate": false,
      "collection": "visits"
    },
    "tourist_activities": {
      "via": "tourist_id",
      "autoPopulate": false,
      "collection": "tourist-activities"
    },
    "tourist_photos": {
      "via": "tourist_id",
      "autoPopulate": false,
      "collection": "tourist-photos"
    },
    "tourist_places": {
      "via": "tourist_id",
      "autoPopulate": false,
      "collection": "tourist-places"
    }
  }
}
